{
    "name": "setono/doctrine-object-manager-trait",
    "description": "A very simple library that offers an Object manager trait to get the object manager for a given class",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/persistence": "^1.3 || ^2.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "infection/infection": "^0.21.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.1 || ^0.17.0",
        "roave/security-advisories": "dev-latest",
        "setono/code-quality-pack": "^2.1"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\DoctrineObjectManagerTrait\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\DoctrineObjectManagerTrait\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "infection/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
